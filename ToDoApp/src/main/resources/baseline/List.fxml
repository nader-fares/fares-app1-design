<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="386.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="baseline.ListController">
    <Button fx:id="addList" layoutX="225.0" layoutY="69.0" mnemonicParsing="false" onAction="#addList" prefWidth="72.0" text="Add List" />
    <Button fx:id="delList" layoutX="225.0" layoutY="336.0" minWidth="43.0" mnemonicParsing="false" onAction="#delList" prefHeight="25.0" text="Delete List" />
    <TextField fx:id="listTitleField" layoutX="23.0" layoutY="69.0" prefHeight="25.0" prefWidth="182.0" promptText="List Title" />
    <ListView fx:id="todoList" layoutX="23.0" layoutY="103.0" prefHeight="223.0" prefWidth="281.0" />
    <Button fx:id="editList" layoutX="133.0" layoutY="336.0" minWidth="43.0" mnemonicParsing="false" onAction="#editList" prefHeight="25.0" prefWidth="72.0" text="Edit List" />
    <Button fx:id="saveButton" layoutX="23.0" layoutY="29.0" mnemonicParsing="false" onAction="#saveTodoList" prefHeight="25.0" prefWidth="72.0" text="Save List" />
    <Button fx:id="loadButton" layoutX="114.0" layoutY="29.0" mnemonicParsing="false" onAction="#loadTodoList" prefHeight="25.0" prefWidth="72.0" text="Load List" />
</AnchorPane>
